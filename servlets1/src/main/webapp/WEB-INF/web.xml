<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>servlets1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>GenericServlet1</servlet-name>
    <servlet-class>com.jspiders.servlets1.servlet.GenericServlet1</servlet-class>
    </servlet>
    
  <servlet>
  <servlet-name>HttpServlet1</servlet-name>
    <servlet-class>com.jspiders.servlets1.servlet.HttpServlet1</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>HttpServlet1</servlet-name>
  <url-pattern>/Http_Servlet1</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>GenericServlet1</servlet-name>
  <url-pattern>/Generic_Servlet1</url-pattern>
  </servlet-mapping>
  
</web-app>